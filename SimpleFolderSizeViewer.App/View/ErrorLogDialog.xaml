<Window x:Class="SimpleFolderSizeViewer.App.ErrorLogDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimpleFolderSizeViewer.App"
        mc:Ignorable="d"
        Title="ErrorLogDialog" Height="450" Width="800" DataContext="{Binding Path=ErrorLog, Source={StaticResource Locator}}">
    <Grid>
        <ListBox ItemsSource="{Binding ErrorLogs}"/>
    </Grid>
</Window>
